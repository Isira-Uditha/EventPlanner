<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eventplanner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UpdateBudget"></activity>
        <activity android:name=".Home" />
        <activity android:name=".BudgetOne" />
        <activity android:name=".BudgetDetails" />
        <activity android:name=".EditBudget" />
        <activity android:name=".AddBudget" />
        <activity android:name=".AddShoppingList" />
        <activity android:name=".EditShoppingList" />
        <activity
            android:name=".EventHome"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".GuestUpdate"
            android:parentActivityName=".GuestEdit" />
        <activity
            android:name=".GuestEdit"
            android:parentActivityName=".GuestHome" />
        <activity android:name=".GuestHome" />
        <activity
            android:name=".GuestAdd"
            android:parentActivityName=".GuestHome" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>