<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/etABPaidA"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AddShoppingList">

    <TextView
        android:id="@+id/textView32"
        android:layout_width="358dp"
        android:layout_height="495dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="52dp"
        android:background="#3C819EAC"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/etASName"
        android:layout_width="276dp"
        android:layout_height="39dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="104dp"
        android:background="#7963DFEF"
        android:hint="Name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="184dp"
        android:text="@string/as_qty"
        android:textColor="#E1040101"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="244dp"
        android:text="@string/as_price"
        android:textColor="#E1040101"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/etASPrice"
        android:layout_width="178dp"
        android:layout_height="34dp"
        android:layout_marginStart="156dp"
        android:layout_marginTop="236dp"
        android:background="#7963DFEF"
        android:hint="Price"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/etASQty"
        android:layout_width="98dp"
        android:layout_height="35dp"
        android:layout_marginStart="208dp"
        android:layout_marginTop="172dp"
        android:background="#7963DFEF"
        android:hint="Qty"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="304dp"
        android:text="@string/b_note"
        android:textColor="#E1040101"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/etASNote"
        android:layout_width="266dp"
        android:layout_height="90dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="344dp"
        android:background="#7963DFEF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button1"
        android:layout_width="110dp"
        android:layout_height="47dp"
        android:layout_marginTop="476dp"
        android:background="#3caac1"
        android:onClick="addSData"
        android:text="Add"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.823"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="68dp"
        android:text="@string/b_name"
        android:textColor="#E1040101"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>